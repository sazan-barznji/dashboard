{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'RegisterForm',\n\n  data() {\n    return {\n      data: {\n        name: null,\n        email: null,\n        password: null,\n        c_password: null\n      }\n    };\n  },\n\n  methods: {\n    async createUsers() {\n      await axios.post('http://127.0.0.1:8000/api/register', this.data).then(res => {\n        this.data = res;\n      }).catch(error => {\n        console.log(error);\n      });\n      this.$router.push('/login');\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgCA;AACA;EACAA,oBADA;;EAEAC;IACA;MACAA;QACAD,UADA;QAEAE,WAFA;QAGAC,cAHA;QAIAC;MAJA;IADA;EAQA,CAXA;;EAYAC;IACA;MACA,kEACAC,IADA,CACAC;QAAA;MAAA,CADA,EAEAC,KAFA,CAEAC;QACAC;MACA,CAJA;MAKA;IACA;;EARA;AAZA","names":["name","data","email","password","c_password","methods","then","res","catch","error","console"],"sourceRoot":"src/components","sources":["RegisterForm.vue"],"sourcesContent":["<template>\n    <div class=\"register container\">\n\n        <form @submit.prevent=\"createUsers\" method=\"post\">\n            <h1>Register</h1>\n            <div>\n                <h5>Register for New Account</h5>\n                <p>thank you for connecting with us, sign up to manage your account</p>\n            </div>\n            <p></p>\n            <div class=\"mb-3\">\n                <label for=\"name\" class=\"form-label\">Name</label>\n                <input type=\"text\" class=\"form-control\" id=\"name\" required v-model=\"data.name\">\n            </div>\n            <div class=\"mb-3\">\n                <label for=\"Email\" class=\"form-label\">Email address</label>\n                <input type=\"email\" class=\"form-control\" id=\"Email\" v-model=\"data.email\" required>\n            </div>\n            <div class=\"mb-3\">\n                <label for=\"password\" class=\"form-label\">Password</label>\n                <input type=\"password\" class=\"form-control\" id=\"password\" required v-model=\"data.password\">\n            </div>\n            <div class=\"mb-3\">\n                <label for=\"c_password\" class=\"form-label\">Confirm Password</label>\n                <input type=\"password\" class=\"form-control\" id=\"c_password\" required v-model=\"data.c_password\">\n            </div>\n            <button type=\"submit\" class=\"btn btn-primary\">Register</button>\n        </form>\n\n    </div>\n</template>\n<script>\nimport axios from 'axios'\nexport default {\n    name: 'RegisterForm',\n    data() {\n        return {\n            data: {\n                name: null,\n                email: null,\n                password: null,\n                c_password: null\n            }\n        }\n    },\n    methods: {\n        async createUsers() {\n            await axios.post('http://127.0.0.1:8000/api/register', this.data)\n                .then((res) => { this.data = res })\n                .catch((error) => {\n                    console.log(error)\n                });\n            this.$router.push('/login')\n        }\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}